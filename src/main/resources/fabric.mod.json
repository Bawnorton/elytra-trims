{
  "schemaVersion": 1,
  "id": "elytra-trims",
  "name": "Elytra Trims",
  "version": "${version}",
  "description": "Lightweight and customizable support for elytra trims, patterns and dyes",
  "authors": [
    "KikuGie"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/elytra-trims",
    "sources": "https://github.com/Kikugie/elytra-trims",
    "issues": "https://github.com/Kikugie/elytra-trims/issues"
  },
  "mixins": [
    "elytratrims.mixins.json",
    "elytratrims-compat.mixins.json"
  ],
  "accessWidener": "elytratrims.accesswidener",
  "icon": "assets/elytratrims/icon.png",
  "license": "LGPL 3.0",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.kikugie.elytratrims.common.ETServerWrapper"
    ],
    "client": [
      "dev.kikugie.elytratrims.client.ETClientWrapper"
    ],
    "modmenu": [
      "dev.kikugie.elytratrims.client.compat.ModMenuCompat"
    ]
  },
  "depends": {
    "fabricloader": ">=0.15.0",
    "minecraft": "${mc}"
  },
  "conflicts": {
    "dashloader": "*",
    "customizableelytra": "*"
  },
  "breaks": {
    "optifabric": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/TBgNUCfryS"
      }
    }
  }
}